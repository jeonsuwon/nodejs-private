generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int        @id @default(autoincrement())
  name     String     @db.VarChar(32)
  email    String     @db.VarChar(128)
  password String     @db.VarChar(128)
}